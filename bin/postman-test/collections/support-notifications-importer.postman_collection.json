							"var failedCount = 0;",
							"data.forEach(function(item, index) { ",
							"    request.body.raw = JSON.stringify(item)",
							"    pm.sendRequest(request, function (err, res) {",
							"        if (res.code!=200 ) {",
							"            console.log(index, res.stream.toString());",
							"            failedCount++",
							"        }else{",
							"            count++",
							"        }",
							"    });",
							"});",
							"",
