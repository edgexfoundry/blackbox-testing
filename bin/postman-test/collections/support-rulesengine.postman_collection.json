          "var schemas = {} ; ",
          "",
          "schemas.rule = {",
          "    \"type\":\"object\",",
          "    \"$schema\":\"http://json-schema.org/draft-03/schema#\",",
          "    \"description\":\"EdgeX Support Rules Engine rule\",",
          "    \"title\":\"rule\",",
          "    \"properties\":{",
          "        \"name\":{",
          "            \"type\":\"string\",",
          "            \"required\":true,",
          "            \"title\":\"name\"",
          "        },",
          "        \"action\":{",
          "            \"type\":\"object\",",
          "            \"properties\":{",
          "                \"device\":{",
          "                    \"type\":\"string\",",
          "                    \"required\":true,",
          "                    \"title\":\"device\"",
          "                },",
          "                \"command\":{",
          "                    \"type\":\"string\",",
          "                    \"required\":true,",
          "                    \"title\":\"command\"",
          "                },",
          "                \"body\":{",
          "                    \"type\":\"string\",",
          "                    \"required\":true,",
          "                    \"title\":\"body\"",
          "                }",
          "            }",
          "        },",
          "        \"condition\":{",
          "            \"type\":\"object\",",
          "            \"properties\":{",
          "                \"device\":{",
          "                    \"type\":\"string\",",
          "                    \"required\":true,",
          "                    \"title\":\"device\"",
          "                },",
          "            \"checks\":{",
          "            \"type\":\"array\",",
          "            \"required\":false,",
          "            \"title\":\"checks\",",
          "            \"items\":{",
          "                \"type\":\"object\",",
          "                \"properties\":{",
          "                    \"parameter\":{",
          "                        \"type\":\"string\",",
          "                        \"required\":false,",
          "                        \"title\":\"parameter\"",
          "                    },",
          "                    \"operand1\":{",
          "                        \"type\":\"string\",",
          "                        \"required\":false,",
          "                        \"title\":\"operand1\"",
          "                    },",
          "                    \"operation\":{",
          "                        \"type\":\"string\",",
          "                        \"required\":false,",
          "                        \"title\":\"operation\"",
          "                    },",
          "                    \"operand2\":{",
          "                        \"type\":\"string\",",
          "                        \"required\":false,",
          "                        \"title\":\"operand2\"",
          "                    }",
          "                }",
          "            },",
          "            \"uniqueItems\":false",
          "            }",
          "            }",
          "        },",
          "        \"log\":{",
          "                \"type\":\"string\",",
          "                \"required\":false,",
          "                \"title\":\"log\"",
          "            }",
          "    }",
          "};",
          "      ",
